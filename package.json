{
  "name": "getdemo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky && prisma generate --schema ./src/infra/database/prisma/schema.prisma",
    "start": "node ./dist/infra/server.js",
    "start:dev": "nest start --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts --max-warnings 0 --cache",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier . --write --cache",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage.enabled=true",
    "test:e2e": "vitest run --config ./vitest.config.e2e.mts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.mts",
    "test:e2e:coverage": "vitest --config ./vitest.config.e2e.mts --coverage.enabled=true",
    "build": "nest build",
    "services": "docker compose -f src/infra/docker-compose.development.yaml --env-file ./.env",
    "services:up": "pnpm services up -d",
    "services:down": "pnpm services down",
    "migrate:dev": "prisma migrate dev --schema ./src/infra/database/prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema ./src/infra/database/prisma/schema.prisma",
    "seed:dev": "nest start --entryFile ./infra/database/prisma/seed/index",
    "seed:deploy": "node ./dist/infra/database/prisma/seed/index.js",
    "postinstall": "pnpm migrate:deploy && pnpm seed:deploy"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "2.0.9",
    "@anatine/zod-openapi": "2.2.6",
    "@fastify/static": "7.0.4",
    "@nestjs/common": "10.3.10",
    "@nestjs/core": "10.3.10",
    "@nestjs/platform-fastify": "10.3.10",
    "@nestjs/swagger": "7.4.0",
    "@prisma/client": "5.18.0",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "fastify": "4.28.0",
    "fastify-multer": "2.0.3",
    "mime-types": "2.1.35",
    "pretty-bytes": "5.6.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@nestjs/cli": "10.4.2",
    "@nestjs/schematics": "10.1.2",
    "@nestjs/testing": "10.3.10",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.0",
    "@types/mime-types": "2.1.4",
    "@types/node": "20.12.12",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vitest": "0.4.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.5",
    "prettier": "3.2.5",
    "prisma": "5.18.0",
    "supertest": "7.0.0",
    "type-fest": "4.23.0",
    "typescript": "5.4.5",
    "unplugin-swc": "1.5.1",
    "vitest": "1.6.0"
  }
}
